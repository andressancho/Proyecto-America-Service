
@{
    ViewBag.Title = "Index";
}
@model  IEnumerable<AmericanService.Models.Historico>

<h2>Histórico de reclutandos</h2>
<br />
@using (Html.BeginForm("Find", "Historico"))
{
    <p>
        Buscar: <input type="text" id="buscar_string" name="buscar_string">
        <input type="submit" value="Aceptar" />
    </p>
}

    <br />
    <body>
        <table class="table table-striped table-bordered table-hover">
            <tr>
                <th>Cédula</th>
                <th>Nombre</th>
                <th>Descripción</th>
                <th>Fecha</th>
                <th>Cantidad</th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.cedula</td>
                    <td>@item.nombre</td>
                    <td>@item.descripcion</td>
                    <td>@item.fecha</td>
                    <td>@item.cantidad</td>

                    <td>
                        @Html.ActionLink("Edit", "Edit", new { cedula = item.cedula })|
                        @Html.ActionLink("Delete", "Delete", new { cedula = item.cedula })
                    </td>
                </tr>
                }

        </table>
    </body>


