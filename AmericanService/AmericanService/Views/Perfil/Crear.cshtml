
@{
    ViewBag.Title = "Crear";
    Layout = "~/Views/Shared/_Menu.cshtml";
}
<head>
    <meta charset='utf-8'>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <script src="http://code.jquery.com/jquery-latest.min.js" type="text/javascript"></script>
</head>
<body>
   


    <h1>Gestión de perfil</h1>
    @using (Html.BeginForm("CrearPerfil", "Perfil"))
    {
        <h3>Cédula: <input type="text" name="cedula"> </h3>
        <h3>Nombre: <input type="text" name="nombre"></h3>
        <h3>Apellidos: <input type="text" name="apellidos"></h3>
        <h3>Cumpleaños: <input type="text" name="fecha_nacimiento"></h3>
        <h3>Usuario: <input type="text" name="usuario"></h3>
        <h3>Contraseña: <input type="password" name="contrasena"></h3>
        <input type="submit" value="Crear">
    }
</body>

